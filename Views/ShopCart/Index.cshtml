@model ShopCartViewModel
<div class="container">
    @foreach (var el in Model.ShopCart.Items)
    {
    <div class="alert alert-warning mt-3">
        <b>Автомобиль:</b> @el.Car.Name<br />
        <b>Цена:</b> @el.Car.Price.ToString("c")<br />
        <img class="img-thumbnail" src="@el.Car.Img" alt="@el.Car.Name" />
    </div>
    }
     
        <a class="btn btn-danger" asp-controller="Order" asp-action="Checkout">Оплатить</a>
        <a class="btn btn-danger" asp-controller="ShopCart" asp-action="Delete">Удалить</a>
    </div>
    